    "mocha": ">=1.18.2",
    "pngjs": ">=0.4.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "tslint": "^5.17.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/core": "^7.5.0"
  }
